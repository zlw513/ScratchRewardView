<?xml version="1.0" encoding="utf-8"?>
<resources>

    
    <declare-styleable name="ScratchRewardView">
        
        <attr name="traceWidth" format="float"/>
        <attr name="maskColor" format="string"/>
        <attr name="uncoverPercent" format="integer"/>
        <attr name="maskBitmap" format="reference"/>

    </declare-styleable>

    <style name="Scratch_Dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
    </style>


</resources>